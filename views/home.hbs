<div class="container">
  <div class="row justify-content-center">
    <h1 class="text-center text-primary">{{pages.books}}</h1>
    {{#each books}}
    <div class="col-3 border m-3 p-3">
      <h2>{{../pages.name}}: {{title}}</h2>
      <hr>
      <h2> {{../pages.pages}} : {{pages}} {{../pages.page}}</h2>
      <hr>
      <h2>{{../pages.amount}}: {{amount}} {{../pages.valute}} </h2>
    </div>
{{/each}}
    </div>
  </div>
</div>